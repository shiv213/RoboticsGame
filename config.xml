<?xml version='1.0' encoding='utf-8'?>
<widget id="com.technotitans.titanpowerup" version="1.6.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TitanPowerUp</name>
    <description>
        Techno Titans Team 1683 Official First PowerUp game.
    </description>
    <author email="northviewrobotics@technotitans.org" href="https://www.technotitans.org">
        Techno Titans
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="https://www.technotitans.org/donate.html" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <platform name="android">
        <preference name="Fullscreen" value="true" />
        <preference name="Orientation" value="landscape" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-fullscreen" spec="^1.2.0" />
    <engine name="android" spec="^7.1.0" />
</widget>

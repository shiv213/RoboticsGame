<?xml version='1.0' encoding='utf-8'?>
<widget id="com.technotitans.titanpowerup" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>TitanPowerUp</name>
    <description>
        Techno Titans Team 1683 Official First PowerUp game.
    </description>
    <author email="northviewrobotics@technotitans.org" href="https://www.technotitans.org">
        Techno Titans
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="https://www.technotitans.org/donate.html" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
</widget>
